
pipeline {

    enviornment {
        aws_access_key = credentials('aws_access_key')
        aws_secret_key = credentials('aws_secret_key')
    }

    agent any

    stages {
        stage('SCM') {
            steps {
                // Get some code from a GitHub repository
                git 'https://github.com/soniya979/terraform_2022.git'

            }
        }

            stage('init') {
                steps {

                    sh terraform init
                }
            }
            
            stage('plan') {
                steps {

                    sh terraform plan
                }
            }
            }
        }